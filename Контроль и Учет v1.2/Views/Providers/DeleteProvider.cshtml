@model Контроль_и_Учет_v1._2.DB_ModelContext.Providers

@{
    ViewBag.Title = "DeleteProvider";
}

<script src="~/Scripts/MyJavaScript/DeleteProviderJavaScript.js"></script>



<h2>Удаление фирмы поставщика</h2>
<hr />

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <form>
        <div class="form-horizontal">

            <div class="form-group">

                <p>Название фирмы поставщика:</p>

                <div class="col-md-10">
                    <div class="form-group">
                        <input id="FirmTitle" class="form-control" />
                    </div>
                </div>
            </div>

            <div class="form-group">

                <p>Фамилия директора:</p>

                <div class="col-md-10">
                    <div class="form-group">
                        <input id="Surname" class="form-control" />
                    </div>
                </div>
            </div>


        </div>

        <div class="col-md-offset-2 col-md-10">
            <input id="DeleteFirm" type="submit" value="Удалить" class="btn-success btn-lg" /> 
        </div>

    </form>
}

    <div> 
    @Html.ActionLink("Вернутся", "Index") 
    </div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
